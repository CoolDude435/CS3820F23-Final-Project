{
  "name" : "CS:3820 F23 Problem Set 9",
  "image" : "docker.io/jgbm/cs3820:latest",
  "customizations": {
      "vscode": {
          "extensions" : ["haskell.haskell", "github.classroom"],
          "settings" : {
              "haskell" : {
                  "manageHLS" : "GHCup",
                  "toolchain" : {
                      "ghc" : "9.6.2", 
                      "hls" : "2.1.0.0",
                      "cabal" : "3.10.1.0",
                      "stack" : null
                  }
              }
          }
      },
      "codespaces": {
          "extensions" : ["haskell.haskell", "github.classroom"],
          "settings" : {
              "haskell" : {
                  "manageHLS" : "GHCup",
                  "toolchain" : {
                      "ghc" : "9.6.2",
                      "hls" : "2.1.0.0",
                      "cabal" : "3.10.1.0",
                      "stack" : null
                  }
              }
          }
      }
  },
  

  "workspaceMount": "source=${localWorkspaceFolder},target=/home/vscode/ps9,type=bind,consistency=delegated",
  "workspaceFolder": "/home/vscode/ps9",

  "remoteUser": "vscode"
}
